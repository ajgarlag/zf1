<?xml version="1.0"?>
<configdata>
    <context type='action' name = 'create' alias = 'c' class='Zend_Build_Action_Create'>
        <attribute getopt='apple|a'>
            <usage>
                This is how you use apple with create.
            </usage>
        </attribute>
        <attribute getopt='banana|b=i'>
            <usage>
                This is how you use banana with create: banana must have an integer param.
            </usage>
        </attribute>
        <attribute getopt='pear|p-s'>
            <usage>
                This is how you use pear with create: pear can have a string param.
            </usage>
        </attribute>
    </context>
</configdata>
